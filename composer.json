{
  "name": "setbased/php-stratum",
  "description": "A stored procedure and function loader, wrapper generator for MySQL in PHP.",
  "keywords": ["MySQL","Stored Routines","Stored Procedures","Stored Functions","Wrapper","Loader","PHP"],
  "type": "tool",
  "license": "MIT",
  "authors": [
    {
      "name": "Set Based IT Consultancy",
      "email": "info@setbased.nl",
      "homepage": "http://www.setbased.nl"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "*",
    "setbased/phing-extensions": "2.*"
  },
  "require": {
    "php": ">=5.4.0",
    "phpdocumentor/reflection-docblock": "2.*"
  },
  "bin": [
    "bin/stratum"
  ],
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\": "src/"
    },
    "classmap": ["test/C.php"]
  },
  "config": {
    "bin-dir": "bin"
  }
}